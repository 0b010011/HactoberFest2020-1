#include<iostream>
#include <bits/stdc++.h>
#include<cmath>
#include <cstdio>
#include <cstring>
using namespace std;

int main()
{
 int t,n;
cin>>t;
while(t--)
{
	cin>>n;
    if(n==1)
    {
	cout<<-1<<"\n";
    continue;
    }  
    cout<<2;
    for(int i=0;i<n-1;i++)
    {
	 cout<<3;
    }
    cout<<"\n";
}	
return 0;
}
