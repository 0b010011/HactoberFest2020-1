#include <bits/stdc++.h>
#include <iostream>  
#include <algorithm>  
#include <vector>  
#include <string>  
#include <cctype>
#include<algorithm>
using namespace std;

int SheenaFlowers(int n,int k,int arr[])
{
	
vector<int> vec;
for(int i=0;i<min(n,k);i++)
{
	
vec.push_back(arr[i]);


}
sort(vec.begin(),vec.end());
int last= unique(vec.begin(),vec.end());
vec.erase(last.vec.end());
return vec.size();

}

int main()
{
	int n=10;
    int k=7;
    int arr[100]={ 2,1,2,1,3,1,4,5,6,1};

    cout<<SheenaFlowers(n,k,arr)<<" ";
												  return 0;
}
