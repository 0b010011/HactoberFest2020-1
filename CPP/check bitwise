#include<bits/stdc++.h>
using namespace std;
int main()
{ios_base::sync_with_stdio(false);
    cin.tie(NULL);
 int t;
    cin>>t;
    while(t--)
    {
	 int x,y,l,r,k;
     cin>>x>>y>>l>>r;
     if (x==0 and y==0)
		 k=0;
     else
        {if (l==0 or r==0)
        k= (x|y)-r;
        else
        k=x|y;}
    cout<<k<<endl;
}
 return 0;
}
